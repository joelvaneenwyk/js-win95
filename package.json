{
  "name": "win95-js",
  "productName": "win95-js",
  "version": "3.1.2",
  "main": "dist/src/main/main.js",
  "author": "Felix Rieseberg, felix@felixrieseberg.com",
  "homepage": "https://microsoft.com",
  "bugs": "https://github.com/joelvaneenwyk/windows95",
  "description": "Windows 95 running in an Electron application.",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^18.20 || ^20.10 || ^21.7 || ^22.3 || >=23",
    "npm": "^10.8"
  },
  "targets": {
    "main": false
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/joelvaneenwyk/windows95"
    }
  ],
  "keywords": [
    "windows",
    "windows95",
    "win95",
    "electron",
    "emulator",
    "retro",
    "retro-computing",
    "retro-computers",
    "retro-gaming"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/joelvaneenwyk/windows95"
  },
  "directories": {
    "output": "./dist"
  },
  "maintainers": [
    {
      "name": "Felix Rieseberg",
      "email": "felix@felixrieseberg.com"
    }
  ],
  "contributors": [
    {
      "name": "Joel Van Eenwyk",
      "email": "joel.vaneenwyk@gmail.com"
    }
  ],
  "scripts": {
    "bundler": "parcel build --dist-dir ./dist --log-level info --public-url ./ --no-optimize --no-content-hash static/index.html",
    "env:pull": "npx --yes dotenv-vault@1.24.0 pull --yes",
    "env:push": "npx --yes dotenv-vault@1.24.0 push --yes",
    "start": "rimraf ./dist && npm run typescript:compile && electron-forge start",
    "prePackage": "npm run typescript:compile",
    "build": "npm run package",
    "package": "npm run typescript:compile && electron-forge package --verbose",
    "make": "electron-forge make --verbose",
    "test": "npm run build && electron-forge test",
    "publish": "electron-forge publish --verbose",
    "lint": "npm run lint:cli && npm run format:check && npm run check-links",
    "lint:cli": "eslint ./src ./tools",
    "lint:fix": "npm run lint:cli -- --fix",
    "format": "npm run lint:fix && npm run format:write",
    "format:cli": "prettier . --trailing-comma none --no-semi",
    "format:write": "npm run format:cli -- --write --log-level debug",
    "format:check": "npm run format:cli -- --check",
    "typescript:compile": "tsc",
    "check-links": "node ./tools/check-links.js"
  },
  "browserslist": "> 0.25%, not dead",
  "config": {
    "forge": "./forge.config.js"
  },
  "overrides": {
    "eslint": "^9.6.0",
    "svgo": "^2.4.0"
  },
  "dependencies": {
    "electron": "31.1.0",
    "electron-squirrel-startup": "^1.0.1",
    "fs-extra": "^11.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/publisher-github": "^7.4.0",
    "@eslint/compat": "^1.1.0",
    "@eslint/js": "^9.6.0",
    "@parcel/config-default": "^2.12.0",
    "@parcel/core": "^2.12.0",
    "@parcel/transformer-html": "^2.12.0",
    "@parcel/transformer-less": "^2.12.0",
    "@parcel/transformer-raw": "^2.12.0",
    "@parcel/transformer-typescript-tsc": "^2.12.0",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "@parcel/types": "^2.12.0",
    "@types/eslint": "^8.56.10",
    "@types/eslint__js": "^8.42.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@typescript-eslint/types": "^7.15.0",
    "@typescript-eslint/utils": "^7.15.0",
    "assert": "^2.1.0",
    "constants-browserify": "^1.0.0",
    "dotenv": "^16.4.5",
    "electron": "31.1.0",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "events": "^3.3.0",
    "globals": "^15.7.0",
    "less": "^4.2.0",
    "node-fetch-commonjs": "^3.3.2",
    "node-gyp": "^10.1.0",
    "parcel": "^2.12.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.3.2",
    "process": "^0.11.10",
    "punycode": "^2.3.1",
    "rimraf": "^5.0.7",
    "stream-browserify": "^3.0.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0",
    "update-electron-app": "^3.0.0",
    "util": "^0.12.5"
  }
}
