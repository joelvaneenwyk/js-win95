{
  "name": "windows95",
  "productName": "windows95",
  "version": "3.1.2",
  "main": "dist/src/main/main.js",
  "source": "src/main/main.ts",
  "types": "dist/types.d.ts",
  "author": "Felix Rieseberg, felix@felixrieseberg.com",
  "homepage": "https://windows.com",
  "bugs": "https://github.com/joelvaneenwyk/windows95",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/joelvaneenwyk/windows95"
    }
  ],
  "keywords": [
    "windows",
    "windows95",
    "electron",
    "emulator",
    "retro",
    "retrocomputing",
    "retrocomputers",
    "retro-gaming"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/joelvaneenwyk/windows95"
  },
  "directories": {
    "output": "./dist"
  },
  "maintainers": [
    {
      "name": "Felix Rieseberg",
      "email": "felix@felixrieseberg.com"
    }
  ],
  "contributors": [
    {
      "name": "Joel Van Eenwyk",
      "email": "joel.vaneenwyk@gmail.com"
    }
  ],
  "description": "Windows 95 running in an Electron application.",
  "license": "MIT",
  "workspaces": [],
  "type": "commonjs",
  "engines": {
    "node": "^20 || ^22"
  },
  "scripts": {
    "env:pull": "npx --yes dotenv-vault@1.24.0 pull --yes",
    "env:push": "npx --yes dotenv-vault@1.24.0 push --yes",
    "start": "rimraf ./dist && electron-forge start",
    "build": "npm run package",
    "build:debug": "DEBUG=electron-windows-installer*,electron-forge* npm run build",
    "package": "electron-forge package --verbose",
    "make": "electron-forge make --verbose",
    "test": "npm run build && electron-forge test",
    "publish": "electron-forge publish --verbose",
    "lint": "npm run lint:cli && npm run format:check && npm run check-links",
    "lint:cli": "eslint .",
    "lint:fix": "npm run lint:cli -- --fix",
    "format": "npm run lint:fix && npm run format:write",
    "format:cli": "prettier . --trailing-comma none --no-semi",
    "format:write": "npm run format:cli -- --write --log-level debug",
    "format:check": "npm run format:cli -- --check",
    "tsc": "tsc -p tsconfig.json --noEmit",
    "check-links": "node tools/check-links.js"
  },
  "browserslist": "> 0.25%, not dead",
  "config": {
    "forge": "./forge.config.js"
  },
  "overrides": {
    "eslint": "^9.6.0"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-flatpak": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/publisher-github": "^7.4.0",
    "@eslint/compat": "^1.1.0",
    "@eslint/js": "^9.6.0",
    "@parcel/config-default": "^2.12.0",
    "@parcel/core": "^2.12.0",
    "@parcel/transformer-html": "^2.12.0",
    "@parcel/transformer-less": "^2.12.0",
    "@parcel/transformer-raw": "^2.12.0",
    "@parcel/transformer-typescript-tsc": "^2.12.0",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "@parcel/types": "^2.12.0",
    "@types/deasync": "^0",
    "@types/eslint": "^8.56.10",
    "@types/eslint__js": "^8.42.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14",
    "@types/node-abi": "^3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/types": "^7.14.1",
    "@typescript-eslint/utils": "^7.14.1",
    "dotenv": "^16.4.5",
    "electron": "31.1.0",
    "electron-squirrel-startup": "^1.0.1",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "fs-extra": "^11.2.0",
    "globals": "^15.6.0",
    "less": "^4.2.0",
    "node-abi": "^3",
    "node-gyp": "^10",
    "parcel": "^2.12.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.14.1",
    "update-electron-app": "^3.0.0"
  }
}
